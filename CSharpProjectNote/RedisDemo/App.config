<?xml version="1.0" encoding="utf-8" ?>
<configuration>

    <appSettings>
        <!--Redis 配置 Begin-->
        <add key="SessionExpireMinutes" value="180"/>
        <add key="redis_server_session" value="127.0.0.1:6379"/>
        <add key="redis_max_read_pool" value="3"/>
